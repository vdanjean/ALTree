Begin data;
dimension ntax=15 nchar=12;
format symbols="0123456789" missing=?;
matrix
H002	112221111122
H010	222212222222
H007	222221111121
H008	112221112112
H014	112221112222
H013	222221122222
H001	112222222222
H003	221122222222
H004	222222222222
H005	221222222221
H012	222221111112
